{
  "required_api_version": "^2.0.0",
  "name": "Jihong",
  "description": "Demo extension",
  "developer_name": "Jihong Ju",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "keyword",
      "type": "keyword",
      "name": "1dictionary",
      "description": "Keyword to trigger 1dictionary",
      "default_value": "ad"
    },
    {
      "id": "online_dictionary",
      "type": "text",
      "name": "Online dictionary",
      "description": "Query for the online dictionary, e.g. Linguee, Youdao, Merriam-Webster",
      "default_value": "https://www.linguee.com/dutch-english/search?source=auto&query=%s"
    }
  ]
}
